{
    "description": "Club Management",
    "authors": [
        {
            "name": "fbraem",
            "email": "franky.braem@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "league/fractal": "0.18.*",
        "nesbot/carbon": "2.25.3",
        "robmorgan/phinx": "^0.11",
        "league/flysystem": "1.1.4",
        "league/commonmark": "^1.4",
        "league/csv": "^9.5",
        "league/plates": "^3.3",
        "ramsey/uuid": "^4",
        "tuupola/slim-jwt-auth": "^3.4",
        "league/container": "^3.3",
        "myclabs/php-enum": "^1.7",
        "swiftmailer/swiftmailer": "^6",
        "league/uri": "^6.4",
        "nette/schema": "^1",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "monolog/monolog": "2.0.2",
        "latitude/latitude": "^4.2",
        "illuminate/collections": "^8.17",
        "relay/relay": "~2.0",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "laminas/laminas-httphandlerrunner": "^1.3",
        "symfony/routing": "^5.2",
        "symfony/psr-http-message-bridge": "^2.0",
        "neomerx/cors-psr7": "^2.0",
        "filp/whoops": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12.8",
        "mockery/mockery": "^1.3",
        "phpmd/phpmd": "2.9.*",
        "squizlabs/php_codesniffer": "3.*",
        "friendsofphp/php-cs-fixer": "2.*",
        "symfony/var-dumper": "^5.2",
        "pestphp/pest": "1.15.0"
    },
    "autoload": {
        "psr-4": {
            "Kwai\\": "src/kwai"
        },
        "files": [
            "src/kwai/macros.php",
            "src/kwai/Core/Infrastructure/Dependencies/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\PHPStan\\": "src/phpstan"
        }
    }
}
